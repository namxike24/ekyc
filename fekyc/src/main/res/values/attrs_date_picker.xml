<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="DatePickerAdvance">

        <attr name="dp_mode" format="enum">
            <enum name="day_month_year" value="1" />
            <enum name="month_year" value="2" />
        </attr>

        <attr name="dp_column_width" format="dimension" />
        <attr name="dp_column_space" format="dimension" />

        <attr name="dp_locale" format="enum">
            <enum name="en" value="0" />
            <enum name="vi" value="1" />
        </attr>

    </declare-styleable>

</resources>
