<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CaptureView">
        <attr name="captureLeftIcon" format="reference" />
        <attr name="captureMiddleIcon" format="reference" />
        <attr name="captureRightIcon" format="reference" />
        <attr name="captureLeftIconSize" format="dimension" />
        <attr name="captureMiddleIconSize" format="dimension" />
        <attr name="captureRightIconSize" format="dimension" />
        <attr format="enum" name="captureCameraFacing">
            <enum name="front" value="0"/>
            <enum name="back" value="1"/>
        </attr>
        <attr format="enum" name="captureFlash">
            <enum name="off" value="0"/>
            <enum name="on" value="1"/>
        </attr>
        <attr format="enum" name="captureLeftIconVisibility">
            <enum name="hide" value="0"/>
            <enum name="show" value="1"/>
        </attr>
        <attr format="enum" name="captureMiddleIconVisibility">
            <enum name="hide" value="0"/>
            <enum name="show" value="1"/>
        </attr>
        <attr format="enum" name="captureRightIconVisibility">
            <enum name="hide" value="0"/>
            <enum name="show" value="1"/>
        </attr>
    </declare-styleable>
</resources>
