<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ToolbarView">
        <attr name="tbv_left_icon_src" format="reference" />

        <attr name="tbv_title_text" format="string" />
        <attr name="tbv_title_text_size" format="dimension" />

        <attr name="tbv_right_text_content" format="string" />
        <attr name="tbv_right_text_text_size" format="dimension" />


        <attr name="tbv_right_icon_src" format="reference" />

        <attr name="tbv_right_type" format="enum">
            <enum name="NONE" value="0"/>
            <enum name="TEXT" value="1"/>
            <enum name="ICON" value="2"/>
        </attr>

    </declare-styleable>

</resources>
